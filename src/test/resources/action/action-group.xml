<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
   

    <package name="group" extends="default" namespace="/group">
        <action name="group_*" class="com.woyo.action.GroupAction"
            method="{1}">
            <result name="goAll">/admin_allGroups.jsp</result>
            <result name="goAdd">/group_add.jsp</result>
            <result name="goUpdateJsp">/group_add.jsp</result>
        </action>
    </package>

  
</struts>